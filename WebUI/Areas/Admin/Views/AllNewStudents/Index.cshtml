											<th scope="col">Father's name</th>
											<th scope="col">Username</th>
											<th scope="col">Password</th>
										</tr>
									</thead>
									<tbody>
										@foreach(var student in Model)
										{
											<tr>

												<td>@Html.DisplayFor(modelItem => student.Name)</td>
												<td>@Html.DisplayFor(modelItem => student.Surname)</td>
												<td>@Html.DisplayFor(modelItem => student.FathersName)</td>
												<td>@Html.DisplayFor(modelItem => student.UserName)</td>
												<td>@Html.DisplayFor(modelItem => student.Password)</td>
											</tr>
										}
										
										
